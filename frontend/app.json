{
  "expo": {
    "name": "letspill",
    "slug": "letspill",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "letspill",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.letspill"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.letspill"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter/Inter18pt-Black.ttf",
            "./assets/fonts/Playwrite_US_Trad/PlaywriteUSTrad-Thin.ttf",
            "./assets/fonts/Playwrite_US_Trad/PlaywriteUSTrad-ExtraLight.ttf",
            "./assets/fonts/Playwrite_US_Trad/PlaywriteUSTrad-Light.ttf",
            "./assets/fonts/Playwrite_US_Trad/PlaywriteUSTrad-Regular.ttf"
          ]
        }
      ],
      "expo-secure-store",
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
